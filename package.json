{
  "name": "@mocks-server/admin-api-client",
  "version": "4.0.0",
  "description": "Client of @mocks-server/plugin-admin-api",
  "keywords": [
    "mocks-server",
    "plugin",
    "administration",
    "admin",
    "api",
    "rest",
    "client"
  ],
  "author": "Javier Brea",
  "license": "MIT",
  "repository": "https://github.com/mocks-server/admin-api-client",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "rollup --config",
    "lint": "eslint *.js test src mocks/*.js mocks/mocks/*.js",
    "lint-staged": "lint-staged",
    "mocks:install": "cd mocks && npm i",
    "mocks:ci": "cd mocks && npm run mocks:ci",
    "test": "jest",
    "test:unit": "npm run mocks:install && start-server-and-test mocks:ci tcp:127.0.0.1:3200 test",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@mocks-server/admin-api-paths": "2.2.0",
    "cross-fetch": "3.1.5"
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.11",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.1.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.5.1",
    "babel-polyfill": "6.26.0",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "27.5.1",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1",
    "rollup": "2.68.0",
    "rollup-plugin-terser": "7.0.2",
    "sinon": "13.0.1",
    "start-server-and-test": "1.14.0"
  },
  "lint-staged": {
    "test/**/*.js": "eslint",
    "src/**/*.js": "eslint",
    "*.js": "eslint",
    "mocks/*.js": "eslint",
    "mocks/mocks/*.js": "eslint"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
