{
  "name": "@mocks-server/admin-api-client",
  "version": "2.0.0",
  "description": "Client of @mocks-server/plugin-admin-api",
  "keywords": [
    "mocks-server-plugin",
    "administration",
    "admin",
    "api",
    "rest",
    "client"
  ],
  "author": "Javier Brea",
  "license": "MIT",
  "repository": "https://github.com/mocks-server/admin-api-client",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "rollup --config",
    "lint": "eslint index.js test src jest.config.js",
    "lint-staged": "lint-staged",
    "mocks:install": "cd mocks && npm i",
    "mocks:ci": "cd mocks && npm run mocks:ci",
    "test": "jest",
    "test:coverage": "npm run mocks:install && start-server-and-test mocks:ci tcp:3200 test",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test:ci": "npm run test:coverage"
  },
  "dependencies": {
    "cross-fetch": "3.0.4",
    "@mocks-server/admin-api-paths": "1.0.4"
  },
  "devDependencies": {
    "@babel/preset-env": "7.8.4",
    "@rollup/plugin-commonjs": "11.0.1",
    "@rollup/plugin-node-resolve": "7.0.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-polyfill": "6.26.0",
    "coveralls": "3.0.9",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.0",
    "husky": "4.2.1",
    "jest": "24.9.0",
    "lint-staged": "10.0.8",
    "prettier": "1.19.1",
    "rollup": "1.31.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-uglify": "6.0.4",
    "sinon": "8.1.1",
    "start-server-and-test": "1.10.8"
  },
  "lint-staged": {
    "test/**/*.js": "eslint",
    "src/**/*.js": "eslint",
    "index.js": "eslint",
    "jest.config.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "10.x || 12.x || 13.x"
  }
}
