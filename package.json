{
  "name": "@mocks-server/admin-api-client",
  "version": "3.0.0",
  "description": "Client of @mocks-server/plugin-admin-api",
  "keywords": [
    "mocks-server",
    "plugin",
    "administration",
    "admin",
    "api",
    "rest",
    "client"
  ],
  "author": "Javier Brea",
  "license": "MIT",
  "repository": "https://github.com/mocks-server/admin-api-client",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "rollup --config",
    "lint": "eslint *.js test src mocks/*.js mocks/mocks/*.js",
    "lint-staged": "lint-staged",
    "mocks:install": "cd mocks && npm i",
    "mocks:ci": "cd mocks && npm run mocks:ci",
    "test": "jest",
    "test:unit": "npm run mocks:install && start-server-and-test mocks:ci tcp:3200 test"
  },
  "dependencies": {
    "cross-fetch": "3.0.6",
    "@mocks-server/admin-api-paths": "2.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.13.12",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-polyfill": "6.26.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.1",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "rollup": "2.44.0",
    "rollup-plugin-terser": "7.0.2",
    "sinon": "9.2.4",
    "start-server-and-test": "1.12.1"
  },
  "lint-staged": {
    "test/**/*.js": "eslint",
    "src/**/*.js": "eslint",
    "*.js": "eslint",
    "mocks/*.js": "eslint",
    "mocks/mocks/*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "12.x || 14.x || 15.x"
  }
}
